<% if (site.data.profile.socials !== undefined) { %>
<div class="social-links">

    <% if (site.data.profile.socials.github !== undefined) { %>
    <a class="social-link github"
       href="<%= site.data.profile.socials.github %>"
       title="پروفایل گیت‌هاب"
       rel="nofollow">
        <i class="fa fa-github-alt"></i>
    </a>
    <% } %>

    <% if (site.data.profile.socials.facebook !== undefined) { %>
    <a class="social-link facebook"
       href="<%= site.data.profile.socials.facebook %>"
       title="صفحه فیسبوک"
       rel="nofollow">
        <i class="fa fa-facebook"></i>
    </a>
    <% } %>

    <% if (site.data.profile.socials.twitter !== undefined) { %>
    <a class="social-link twitter"
       href="<%= site.data.profile.socials.twitter %>"
       title="صفحه توییتر"
       rel="nofollow">
        <i class="fa fa-twitter"></i>
    </a>
    <% } %>

    <% if (site.data.profile.socials.googleplus !== undefined) { %>
    <a class="social-link googleplus"
       href="<%= site.data.profile.socials.googleplus %>"
       title="صفحه گوگل‌پلاس"
       rel="nofollow">
        <i class="fa fa-google-plus"></i>
    </a>
    <% } %>

    <% if (site.data.profile.socials.linkedin !== undefined) { %>
    <a class="social-link linkedin"
       href="<%= site.data.profile.socials.linkedin %>"
       title="لینکداین"
       rel="nofollow">
        <i class="fa fa-linkedin"></i>
    </a>
    <% } %>

    <% if (site.data.profile.socials.telegram !== undefined) { %>
    <a class="social-link telegram"
       href="<%= site.data.profile.socials.telegram %>"
       title="تلگرام"
       rel="nofollow">
        <i class="fa fa-telegram"></i>
    </a>
    <% } %>

</div>
<% } %>